// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}

model User {
  id        Int     @id @default(autoincrement())
  name      String? //for some reason It must in this model
  email     String  @unique
  username  String
  password  String
}

model Session {
  id  String    @id @default(uuid())
  sessionID   String @unique
  userID      Int    
  expiration  Int? 
}


